{
  "name": "cloud-etl-stepfunctions",
  "version": "0.1.0",
  "bin": {
    "cloud-etl-stepfunctions": "bin/cloud-etl-stepfunctions.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "AWS_PROFILE=promie npx cdk deploy --all --require-approval never",
    "diff": "AWS_PROFILE=promie npx cdk diff",
    "synth": "AWS_PROFILE=promie npx cdk synth",
    "destroy": "AWS_PROFILE=promie npx cdk destroy --all --force",
    "clean": "rm -rf cdk.out && find app infra -type f \\( -name '*.js' -o -name '*.d.ts' \\) -delete"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "aws-cdk": "2.1033.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.230.0",
    "constructs": "^10.0.0"
  }
}