{
  "name": "cloud-etl-stepfunctions",
  "version": "0.1.0",
  "bin": {
    "cloud-etl-stepfunctions": "bin/cloud-etl-stepfunctions.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "AWS_PROFILE=promie npx cdk deploy --all --require-approval never",
    "diff": "AWS_PROFILE=promie npx cdk diff",
    "synth": "AWS_PROFILE=promie npx cdk synth",
    "destroy": "AWS_PROFILE=promie npx cdk destroy --all --force",
    "clean": "rm -rf cdk.out && find app infra -type f \\( -name '*.js' -o -name '*.d.ts' \\) -delete",
    "lint": "eslint .",
    "prettier:check": "prettier --check \"./**/*.{js,ts,tsx,json,css}\"",
    "prettier:write": "prettier --write \"./**/*.{js,ts,tsx,json,css}\""
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "aws-cdk": "2.1033.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "dependencies": {
    "aws-cdk-lib": "2.230.0",
    "constructs": "^10.0.0"
  }
}
